{
  "name": "hamqsl-mailconfirm",
  "version": "1.0.0",
  "description": "Physical QSL Card Mail Confirmation System with HMAC-based token verification",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "check-deploy": "bash scripts/check-deployment.sh",
    "check-db": "node scripts/init-database.js",
    "generate-secret": "node -e \"console.log('\\nGenerated QSL_TOKEN_SECRET:\\n' + require('crypto').randomBytes(32).toString('hex') + '\\n')\""
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "nanoid": "^5.1.6",
    "next": "16.0.1",
    "next-intl": "^4.4.0",
    "qrcode": "^1.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  }
}
